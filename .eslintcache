();\n        this.setState({\n            currentOffre: null,\n            currentIndex: -1\n        });\n    }\n\n    setActiveOffre(offre, index) {\n        this.setState({\n            currentOffre: offre,\n            currentIndex: index\n        });\n    }\n\n    removeAllOffres() {\n        OffreDataService.deleteAll()\n            .then(response => {\n                console.log(response.data);\n                this.refreshList();\n            })\n            .catch(e => {\n                console.log(e);\n            });\n    }\n\n    searchTitle() {\n        OffreDataService.findByTitle(this.state.searchTitle)\n            .then(response => {\n                this.setState({\n                    offres: response.data\n                });\n                console.log(response.data);\n            })\n            .catch(e => {\n                console.log(e);\n            });\n    }\n\n    render() {\n        const { tel, searchTitle, offres, currentOffre, currentIndex } = this.state;\n\n        return (\n            <div className=\"list row\">\n                <div className=\"col-md-8\">\n                    <div className=\"input-group mb-3\">\n                        <input\n                            type=\"text\"\n                            className=\"form-control\"\n                            placeholder=\"Recherche par mots clés\"\n                            value={searchTitle}\n                            onChange={this.onChangeSearchTitle}\n                        />\n                        <div className=\"input-group-append\">\n                            <button\n                                className=\"btn btn-outline-secondary\"\n                                type=\"button\"\n                                onClick={this.searchTitle}\n                            >\n                                Chercher\n                            </button>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"col-md-6\">\n                    <h4>Offres des boss</h4>\n\n                    <ul className=\"list-group\">\n                        {offres &&\n                        offres.map((offre, index) => (\n                            <li\n                                className={\n                                    \"list-group-item \" +\n                                    (index === currentIndex ? \"active\" : \"\")\n                                }\n                                onClick={() => this.setActiveOffre(offre, index)}\n                                key={index}\n                            >\n                                {offre.title}\n                            </li>\n                        ))}\n                    </ul>\n\n                </div>\n                <div className=\"col-md-6\">\n                    {currentOffre ? (\n                        <div>\n                            <br/>\n                            <h4>Détails de l'offre</h4>\n                            <Link to={\"tel/\" + currentOffre.userId } className=\"badge badge-info\">\n                            Téléphone du Boss\n                        </Link>\n                            <div>\n                                <label>\n                                    <strong>Mots clés:</strong>\n                                </label>{\" \"}\n                                {currentOffre.title}\n                            </div>\n                            <div>\n                                <label>\n                                    <strong>Description:</strong>\n                                </label>{\" \"}\n                                {currentOffre.description}\n                            </div>\n                            <div>\n                                <label>\n                                    <strong>Status:</strong>\n                                </label>{\" \"}\n                                {currentOffre.stock ? \"En stock\" : \"Expiré\"}\n                            </div>\n                        </div>\n                    ) : (\n                        <div>\n                            <br />\n                            <p>Merci de cliquer sur une offre...</p>\n                        </div>\n                    )}\n                </div>\n\n            </div>\n\n\n\n        );\n    }\n}","/Users/alikhedji/Sites/gangsta-front/src/services/offre.service.js",[],"/Users/alikhedji/Sites/gangsta-front/src/http-common.js",[],["88","89"],"/Users/alikhedji/Sites/gangsta-front/src/components/offre.component.js",[],"/Users/alikhedji/Sites/gangsta-front/src/components/board-boss.component.js",[],"/Users/alikhedji/Sites/gangsta-front/src/components/board-lambda.component.js",[],"/Users/alikhedji/Sites/gangsta-front/src/components/tel-display.component.js",[],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"90","replacedBy":"94"},{"ruleId":"92","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":3,"column":9,"nodeType":"98","messageId":"99","endLine":3,"endColumn":13},{"ruleId":"96","severity":1,"message":"100","line":3,"column":15,"nodeType":"98","messageId":"99","endLine":3,"endColumn":20},{"ruleId":"96","severity":1,"message":"101","line":3,"column":22,"nodeType":"98","messageId":"99","endLine":3,"endColumn":28},{"ruleId":"96","severity":1,"message":"102","line":4,"column":8,"nodeType":"98","messageId":"99","endLine":4,"endColumn":11},{"ruleId":"96","severity":1,"message":"103","line":106,"column":17,"nodeType":"98","messageId":"99","endLine":106,"endColumn":20},{"ruleId":"90","replacedBy":"104"},{"ruleId":"92","replacedBy":"105"},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],["106"],["107"],"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","'Route' is defined but never used.","'Switch' is defined but never used.","'Tel' is defined but never used.","'tel' is assigned a value but neve